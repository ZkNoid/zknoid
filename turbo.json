{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".next/**",
        "!.next/cache/**",
        "dist",
        "../../protokit-framework/**",
        "protokit-framework/**"
      ]
    },
    "build^store": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".next/**",
        "!.next/cache/**",
        "dist",
        "../../protokit-framework/**",
        "protokit-framework/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start-server": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "start": {
      "cache": false
    }
  }
}